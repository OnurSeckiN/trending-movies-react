{
  "env": {
    "browser": true,
    "es2020": true
  },
  "extends": ["react-app", "prettier"],
  "parserOptions": {
    "ecmaVersion": 11
  },
  "plugins": ["react", "prettier"],
  "rules": {
    "eqeqeq":"warn",
    "prettier/prettier": [
      "error",
      {
        "printWidth": 120,
        "useTabs": false,
        "tabWidth": 2,
        "singleQuote": true,
        "jsxSingleQuote": true,
        "semi": false,
        "trailingComma": "es5",
        "bracketSpacing": true
      }
    ]
  }
}
